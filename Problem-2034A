#include<stdio.h> // accepted 

int main() {
	int t ;
	scanf("%d", &t);
	while(t--){
	    int a, b, gcd; 
	    scanf("%d %d \n", &a, &b);
	    for(int i = 1; i<= a && i<= b ; ++i){
	        if(a%i == 0 && b%i == 0){
	            gcd = i;
	        }
	    }
	    int x = (a*b) / gcd;
	    printf("%d\n", x);
	}
	return 0;

}

