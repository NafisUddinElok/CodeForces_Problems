#include <stdio.h>

int main() {
    int t;  // number of test cases
    scanf("%d", &t);

    while (t--) {
        int n, x;
        scanf("%d %d", &n, &x);

        int sum = 0;
        int a[n];

        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);
            sum += a[i];
        }
        if (sum == n * x) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
